files:
  "/etc/nginx/conf.d/00_elastic_beanstalk_proxy.conf":
    mode: "000755"
    owner: root
    group: root
    content: |
      server {
        listen   80;
        server_name    lawzle.com;   
        if ($http_x_forwarded_proto = 'http') {
             return 301 https://$server_name$request_uri;   
        }
      }